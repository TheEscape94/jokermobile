{% extends 'adminbase.html.twig' %}
{% block title %} | Title {% endblock %}
{% block body %}
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12 col-lg-12">
            <center><h3>Pretrage koje su korisnici trazili</h3></center>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
            <div class="row">
                <div class="col-md-6 col-md-6 col-sm-12 col-xs-12 col-lg-push-3 col-md-push-3">
                    <div class="x_panel">
                        <div class="x_title">
                            <ul class="nav navbar-right panel_toolbox">
                                <li class="pull-right"><a class="collapse-link"><i class="fa fa-chevron-up"></i></a></li>
                            </ul>
                            <h3>Pretrage <small></small></h3>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content">
                            {% for s in searches|reverse %}
                                <article class="media event">
                                    <p>{{ s.search }} <span class="label label-info">{{ s.createdAt|date('d-m-Y h:m') }}</span></p>
                                </article>
                            {% endfor %}
                            <hr/>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
{% endblock %}