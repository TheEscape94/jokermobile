{% extends 'base.html.twig' %}

{% block body %}
    <h1>Categories_phone</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ categories_phone.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ categories_phone.name }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if categories_phone.createdAt %}{{ categories_phone.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Enabled</th>
                <td>{% if categories_phone.enabled %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_kategorije_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_kategorije_edit', { 'id': categories_phone.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
